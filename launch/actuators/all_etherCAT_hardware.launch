<launch>
    <include file="$(find amigo_machine_files)/machines/amigo1.machine" />
    <include file="$(find amigo_machine_files)/machines/amigo2.machine" />
    <include file="$(find amigo_machine_files)/machines/amigo3.machine" />
    
    <node name="amigo_etherCAT_hardware" pkg="ocl" type="deployer-gnulinux" args="
    -s $(find amigo_etherCAT)/hardware_ROStopics.ops 
    -s $(find amigo_etherCAT)/soem.ops 
    -s $(find amigo_base_controller)/base_ROStopics.ops 
    -s $(find amigo_base_controller)/base_controller.ops
    -s $(find amigo_spindle_controller)/spindle_ROStopics.ops 
    -s $(find amigo_spindle_controller)/spindle_controller.ops 
    -s $(find dynamixel_ethercat)/head_ROStopics.ops 
    -s $(find dynamixel_ethercat)/deploy_head.ops 
    -s $(find rgb_lights)/rgb_ROStopics.ops 
    -s $(find rgb_lights)/deploy_lights.ops 
    --" output="screen" machine="amigo2"></node>   
</launch>
