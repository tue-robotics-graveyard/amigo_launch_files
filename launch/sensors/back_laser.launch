<?xml version="1.0"?>

<launch>

        <include file="$(find amigo_machine_files)/machines/amigo3.machine" />

        <node pkg="hokuyo_node" type="hokuyo_node" name="back_hokuyo_node" output="screen" machine="amigo3">
            <remap from="scan" to="/amigo/base_back_laser"/>
            <param name="frame_id" value="/amigo/back_laser"/>
        </node>

        <node name="$(anon dynparam)" pkg="dynamic_reconfigure" type="dynparam" args="set_from_parameters back_hokuyo_node" machine="amigo3">
            <param name="min_ang" type="double" value="-2.0" />
            <param name="max_ang" type="double" value="2.0" />
        </node>

</launch>
  
