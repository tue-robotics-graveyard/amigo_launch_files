	<param name="xmax" value="10" />
	<param name="ymax" value="10" />
<!--	<param name="srr"  value="10" />	
	<param name="srt"  value="10" />
	<param name="str"  value="10" />
	<param name="stt"  value="10" /> -->
	<param name="angularUpdate"  value="0.5" />

    </node>

</launch>



