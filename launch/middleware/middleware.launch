	<!-- Navigation -->
	<include file="$(find amigo_launch_files)/launch/middleware/base_navigation.launch" />
	
	<!-- Arm & gripper control -->
	<include file="$(find amigo_launch_files)/launch/middleware/arms.launch" />
	
	<!-- Head control -->
